datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Users {
  id          String   @id @default(uuid())
  name        String
  email       String   @unique
  phone       String
  created_at  DateTime @default(now()) 
  updated_at  DateTime @updatedAt
}

model Photos {
  id          String   @id @default(uuid())
  Accessories Json?
  Events      Json?
}
{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Arlindo Boa",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/index.ts",
    "preprod": "npx prisma migrate deploy --preview-feature",
    "prod": "npx prisma generate",
    "postprod": "tsc && node ./dist/index.js",
    "migration:save": "npx prisma migrate dev --name create-tables --preview-feature",
    "migration:revert": "prisma migrate down 1 --experimental",
    "migration:reset": "prisma migrate reset --preview-feature",
    "build": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^2.16.1",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "prisma": "^2.16.1",
    "uuid": "^8.3.2",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2"
  }
}
